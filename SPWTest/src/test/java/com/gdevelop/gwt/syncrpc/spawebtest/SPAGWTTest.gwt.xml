<?xml version="1.0" encoding="UTF-8"?>
<!-- When updating your version of GWT, you should also update this DTD reference, 
	so that your app can take advantage of the latest GWT module capabilities. -->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.0//EN"
  "http://google-web-toolkit.googlecode.com/svn/tags/2.5.0/distro-source/core/src/gwt-module.dtd">
<module>
	<!-- Inherit our applications main module. -->
	<inherits name='com.gdevelop.gwt.syncrpc.spawebtest.SPAWebTest' />
	<inherits name="com.google.gwt.user.RPCSuite" />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
	<!-- Specify the path to any remote services. -->
	<servlet path="/spawebtest/greet"
		class="com.gdevelop.gwt.syncrpc.spawebtest.server.GreetingServiceImpl" />
	<servlet path="/rpcsuite/profile"
		class="com.gdevelop.gwt.syncrpc.spawebtest.server.ProfileServiceImpl" />
	<servlet path="/rpcsuite/largePayload"
		class="com.gdevelop.gwt.syncrpc.spawebtest.server.LargePayloadServiceImpl" />
	<servlet path="/spawebtest/rpcsuite/enums"
		class="com.google.gwt.user.server.rpc.EnumsTestServiceImpl" />
	<servlet path="/spawebtest/customfieldserializers"
		class="com.google.gwt.user.server.rpc.CustomFieldSerializerTestServiceImpl" />
	<servlet path="/spawebtest/exceptions"
		class="com.google.gwt.user.server.rpc.ExceptionsTestServiceImpl" />
	<servlet path="/spawebtest/inheritance"
		class="com.google.gwt.user.server.rpc.InheritanceTestServiceImpl" />
	<servlet path="/spawebtest/objectgraphs"
		class="com.google.gwt.user.server.rpc.ObjectGraphTestServiceImpl" />
	<servlet path="/spawebtest/echo"
		class="com.google.gwt.user.server.rpc.MixedSerializableEchoServiceImpl" />
	<servlet path="/spawebtest/typecheckedobjects"
		class="com.google.gwt.user.server.rpc.TypeCheckedObjectsTestServiceImpl" />
	<servlet path="/spawebtest/unicodeEscape"
		class="com.google.gwt.user.server.rpc.UnicodeEscapingServiceImpl" />
	<servlet path="/spawebtest/xsrftestservice"
		class="com.google.gwt.user.server.rpc.XsrfTestServiceImpl" />
	<servlet path="/spawebtest/valuetypes"
		class="com.google.gwt.user.server.rpc.ValueTypesTestServiceImpl" />
</module>
